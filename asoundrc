pcm.zip {
	type multi
	slaves.a.pcm "hw:1,0"
	slaves.a.channels 64
	slaves.b.pcm "hw:2,0"
	slaves.b.channels 64
	bindings.0.slave a;
	bindings.0.channel 62;
	bindings.1.slave b;
	bindings.1.channel 62;
}

